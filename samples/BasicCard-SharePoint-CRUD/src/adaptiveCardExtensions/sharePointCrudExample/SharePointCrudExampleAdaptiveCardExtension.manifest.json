  "requiresCustomScript": false,
  "supportedHosts": ["Dashboard"],
  "supportsThemeVariants": true,
  "preconfiguredEntries": [{
    "groupId": "bd067b1e-3ad5-4d5d-a5fe-505f07d7f59c", // Dashboard
    "group": {
      "default": "Dashboard"
    },
    "title": {
      "default": "SharePoint CRUD ACE Sample"
    },
    "description": {
      "default": "This is a sample that shows how to work with form fields using Adaptive Card Extensions and integrate with SharePoint lists using PnPJS."
    },
    "officeFabricIconFontName": "SharePointLogo",
    "cardSize": "Large",
    "properties": {
      "title": "SharePoint CRUD ACE Sample",
      "description": "This is a sample that shows how to work with form fields using Adaptive Card Extensions and integrate with SharePoint lists using PnPJS.",
      "iconProperty": "" // Default to sharepointlogo
    }
  }]
