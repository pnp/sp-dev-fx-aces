{
  "schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.2",
  "body": [
    {
      "type": "ColumnSet",
      "$data": "${offices}",
      "separator": true,
      "columns": [
        {
          "type": "Column",
          "width": "stretch",
          "items": [
            {
              "type": "TextBlock",
              "text": "${name}",
              "horizontalAlignment": "Left",
              "size": "medium",
              "spacing": "medium",
              "wrap": true
            },
            {
              "type": "TextBlock",
              "text": "${address}",
              "horizontalAlignment": "Left",
              "spacing": "None",
              "size": "default",
              "isSubtle": true,
              "color": "dark",
              "wrap": true
            }
          ]
        },
        {
          "type": "Column",
          "width": "auto",
          "verticalContentAlignment": "center",
          "items": [
            {
              "type": "ActionSet",
              "horizontalAlignment": "center",
              "actions": [
                {
                  "type": "Action.Submit",
                  "iconUrl": "data:image/png;base64,",
                  "style":"positive",
                  "data": {
                    "uniqueId": "${uniqueId}",
                    "action": "click"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}