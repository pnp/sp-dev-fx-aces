{
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "type": "AdaptiveCard",
    "version": "1.3",
    "body": [
        {
            "type": "Container",
            "$data": "${people}",
            "items": [
                {
                    "type": "TextBlock",
                    "size": "Medium",
                    "text": "${title}",
                    "wrap": true,
                    "weight": "Bolder"
                }
            ]
        }
    ]
}